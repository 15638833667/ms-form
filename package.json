{
  "name": "ms-form",
  "version": "0.1.3",
  "private": false,
  "type": "module",
  "keywords": [
    "vue3",
    "vite"
  ],
  "license": "ISC",
  "author": {
    "name": "ms-form",
    "email": "ms-form@test.com"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "browser": "./dist/index.js",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    "./css": "./dist/style.css",
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "docs:dev": "vitepress dev docs --port 9000 --open",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build": "run-s clean build:lib",
    "clean": "rimraf ./dist",
    "build:lib": "run-p type-check build:es build:browser",
    "type-check": "vue-tsc --noEmit",
    "build:es": "vite build",
    "build:browser": "vite build",
    "pub": "sh scripts/publish.sh ${npm_config_commit}",
    "push": "sh scripts/push.sh",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vueuse/core": "^10.9.0",
    "core-js": "^3.36.0",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "element-plus": "^2.7.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.22.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "typescript": "^4.3.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.9.0",
    "vitepress": "^1.0.1",
    "vue-tsc": "^2.0.6"
  },
  "description": "基于vue3 + vite的组件库模板",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/themusecatcher/ms-form.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
